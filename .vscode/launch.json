{
    "version": "0.2.0",
    "configurations": [
  
      {
        "name": "Start App (Click)",
        "type": "debugpy",
        "request": "launch",
        "program": "${workspaceFolder}/main.py",
        "console": "integratedTerminal",
        "args": ["start-app"],
        "python": "${command:python.interpreterPath}"
      },
      {
        "name": "Start Postgres (Click)",
        "type": "debugpy",
        "request": "launch",
        "program": "${workspaceFolder}/main.py",
        "console": "integratedTerminal",
        "args": ["start-postgres"],
        "python": "${command:python.interpreterPath}"
      },
      {
        "name": "Stop Postgres (Click)",
        "type": "debugpy",
        "request": "launch",
        "program": "${workspaceFolder}/main.py",
        "console": "integratedTerminal",
        "args": ["stop-postgres"],
        "python": "${command:python.interpreterPath}"
      },
      {
        "name": "Debug App (Click)",
        "type": "debugpy",
        "request": "launch",
        "program": "${workspaceFolder}/main.py",
        "console": "integratedTerminal",
        "args": ["debug-app"],
        "python": "${command:python.interpreterPath}"
      }
    ]
  }
  